#!/usr/bin/env python
# coding=utf-8

import sys, pyperclip

def face(t):
    return {
        'smug': 'ðŸ˜',
        'wink': 'ðŸ˜‰',
        'wink tongue': 'ðŸ˜œ',
        'XD': 'ðŸ˜†',
        'frown': 'ðŸ™'
    }.get(t, 'ðŸ˜Š')

f = None
if len(sys.argv) > 1:
    f = " ".join(sys.argv[1:])

pyperclip.copy(face(f))
